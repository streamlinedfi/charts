    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "polished": "^4.3.1",
    "react-drag-listview": "^2.0.0",
    "react-konva": "^19.2.0",
    "react-konva-utils": "^2.0.0",
    "react-outside-click-handler": "^1.3.0",
    "use-image": "^1.1.4"
  },
  "devDependencies": {
    "esbuild-visualizer": "^0.7.0",
    "patch-package": "^8.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
