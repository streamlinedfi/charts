{
  "name": "@streamlinedfi/charts",
  "version": "0.1.0",
  "description": "Charting Library with Financial Candlesticks Chart that provides an alternative to TradingView",
  "author": "Streamlined Finance",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamlinedfi/charts.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamlinedfi/charts/issues"
  },
  "homepage": "https://github.com/streamlinedfi/charts#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup",
    "dev": "tsup --watch",
    "analyze": "npx esbuild-visualizer --metadata ./dist/metafile-esm.json"
