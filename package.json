  "dependencies": {
    "@debut/indicators": "^1.3.22",
    "@streamlinedfi/div": "^0.2.1",
    "@styled-icons/boxicons-regular": "^10.47.0",
    "@styled-icons/evaicons-solid": "^10.46.0",
    "@styled-icons/fa-solid": "^10.47.0",
    "@styled-icons/feather": "^10.47.0",
    "@styled-icons/octicons": "^10.47.0",
    "d3-dispatch": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "deepmerge": "^4.3.1",
    "file-extension": "^4.0.5",
    "friendly-numbers": "^1.0.1",
    "konva": "^10.0.8",
    "lodash": "^4.17.21",
    "milliseconds": "^1.0.3",
